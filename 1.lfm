                <template name="TePopUpExp">
                    <layout align="top" height="20">
                        <label text="$(titulo):" align="left" width="50" horzTextAlign="trailing" />
                        <label field="$(conteudo)" align="right" horzTextAlign="trailing" width="50" fontColor="#000000"/>
                    </layout>
                </template>
                <template name="TePopUpExp1">
                    <layout align="top" height="20">
                        <label text="$(titulo):" align="left" width="50" horzTextAlign="trailing" fontColor="#696969" />
                        <label field="$(conteudo)" align="right" horzTextAlign="trailing" width="50" fontColor="#696969" />
                    </layout>
                </template>
                <template name="TePopUpExp2">
                    <layout align="top" height="20">
                        <label text="$(titulo):" align="left" width="50" horzTextAlign="trailing" fontColor="#696969" />
                        <edit field="$(valor)" align="right" horzTextAlign="trailing" width="300" transparent="true" fontColor="#696969" />
