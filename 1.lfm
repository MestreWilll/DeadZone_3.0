                </template>
                <template name="TePopUpExp2B">
                    <layout align="top" height="20">
                        <label text="$(titulo):" align="left" width="50" horzTextAlign="trailing" fontColor="#000000" />
                        <edit field="$(valor)" align="right" horzTextAlign="trailing" width="300" transparent="true" fontColor="#000000" />
                    </layout>
